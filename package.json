{
    "name": "prohhhgd",
    "version": "3.1.4",
    "author": "Herve de CHAVIGNY",
    "homepage": "http://rvdechavigny.fr/",
    "private": true,
    "main": "main.js",
    "scripts": {
        "clean": "ionic-app-scripts clean",
        "build": "ionic-app-scripts build",
        "lint": "ionic-app-scripts lint",
        "ionic:build": "ionic-app-scripts build",
        "ionic:serve": "ionic-app-scripts serve",
        "electron": "ionic-app-scripts build && electron .",
        "start": "electron .",
        "build:osx": "build --mac --x64",
        "build:win": "build --windows --ia32 --x64"
    },
    "dependencies": {
        "@angular/common": "5.0.3",
        "@angular/compiler": "5.0.3",
        "@angular/compiler-cli": "5.0.3",
        "@angular/core": "5.0.3",
        "@angular/forms": "5.0.3",
        "@angular/http": "^5.2.10",
        "@angular/platform-browser": "^5.2.10",
        "@angular/platform-browser-dynamic": "^5.2.10",
        "@ionic-native/core": "4.5.2",
        "@ionic-native/splash-screen": "^4.7.0",
        "@ionic-native/status-bar": "4.5.2",
        "@ionic/pro": "1.0.16",
        "@ionic/storage": "2.1.3",
        "angular-highcharts": "^5.2.12",
        "cordova-android": "^6.4.0",
        "cordova-plugin-device": "^2.0.2",
        "cordova-plugin-ionic": "^4.1.5",
        "cordova-plugin-ionic-webview": "^1.2.0",
        "cordova-plugin-splashscreen": "^5.0.2",
        "cordova-plugin-whitelist": "^1.3.3",
        "highcharts": "^6.1.0",
        "ionic-angular": "3.9.2",
        "ionic-plugin-keyboard": "^2.2.1",
        "ionicons": "3.0.0",
        "rxjs": "5.5.2",
        "sw-toolbox": "3.6.0",
        "zone.js": "0.8.18"
    },
    "devDependencies": {
        "@ionic/app-scripts": "3.1.7",
        "@types/highcharts": "^5.0.22",
        "electron": "^1.8.4",
        "electron-builder": "^19.56.2",
        "ssh2-sftp-client": "^2.1.1",
        "typescript": "2.4.2"
    },
    "build": {
        "appId": "com.vevedh.prohhhgd",
        "copyright": "Copyright © 2017 Herve de CHAVIGNY",
        "productName": "proHHHGD",
        "files": [
            "www/**/*",
            "node_modules/**/*",
            "main.js",
            "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme,test,__tests__,tests,powered-test,example,examples,*.d.ts}",
            "!**/node_modules/electron",
            "!**/node_modules/.bin",
            "!**/*.{o,hprof,orig,pyc,pyo,rbc}",
            "!**/._*",
            "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,__pycache__,thumbs.db,.gitignore,.gitattributes,.editorconfig,.flowconfig,.yarn-metadata.json,.idea,appveyor.yml,.travis.yml,circle.yml,npm-debug.log,.nyc_output,yarn.lock,.yarn-integrity}"
        ],
        "win": {
            "icon": "www/assets/icon/icon256.ico"
        },
        "nsis": {
            "oneClick": false,
            "installerIcon": "www/assets/icon/icon256.ico"
        }
    },
    "description": "Application de gestion du SI à HO HIO HEN Grande Distribution",
    "cordova": {
        "plugins": {
            "ionic-plugin-keyboard": {},
            "cordova-plugin-ionic": {
                "APP_ID": "d5cc80b5",
                "CHANNEL_NAME": "Production",
                "UPDATE_METHOD": "background",
                "WARN_DEBUG": "true",
                "UPDATE_API": "https://api.ionicjs.com",
                "MAX_STORE": "2"
            },
            "cordova-plugin-splashscreen": {},
            "cordova-plugin-device": {},
            "cordova-plugin-ionic-webview": {},
            "cordova-plugin-whitelist": {}
        },
        "platforms": [
            "android"
        ]
    }
}